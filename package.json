  "scripts": {
    "prebuild": "rimraf -G dist",
    "build": "swc --ignore \\\"**/*.spec.ts,src/hmr.ts\\\" --delete-dir-on-start --out-dir dist/ src/",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "cross-env NODE_OPTIONS=--max-old-space-size=8192 HMR=true node --trace-warnings -r @swc/register src/hmr.ts ",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:generate": "npx prisma generate",
    "docker:build": "docker build -t enime/api:1.0 .",
    "docker:push": "docker push enime/api:1.0"
  },
  "dependencies": {
